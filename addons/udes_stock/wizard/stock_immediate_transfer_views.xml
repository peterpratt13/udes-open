<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_immediate_transfer" model="ir.ui.view">
        <field name="name">stock.immediate.transfer.view.form.udes_stock</field>
        <field name="model">stock.immediate.transfer</field>
        <field name="inherit_id" ref="stock.view_immediate_transfer"/>
        <field name="arch" type="xml">
            <xpath expr="//p" position="replace">
                <p>You have not recorded <i>done</i> quantities yet, by clicking on <i>apply</i> all outstanding quantities will be processed.</p>
            </xpath>
        </field>
    </record>
</odoo>
